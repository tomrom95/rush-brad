{
  "rules": {
    "whitelist": {
      ".read": "false",
      ".write": "false"
    },
    "users": {
      "$uid": {
        ".read": "auth != null",
        ".write": "auth != null"
      }
    },
    "user-profiles": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "rushees": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": "isCut"
    },
    "comments": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "feed": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": "date"
    },
    "settings": {
      ".read": "auth != null",
      ".write": "false"
    },
    "events": {
      ".read": "auth != null",
      ".write": "false"
    }
  }
}
